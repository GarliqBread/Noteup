{
  "name": "@noteup/web",
  "version": "0.4.3",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elementsinteractive/Noteup"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "start": "vite",
    "build": "eslint src && vite build",
    "test": "jest --config jest.config.ts",
    "format": "prettier --write \"./**/*.{json,ts,tsx,js,md}\"",
    "lint": "eslint . --ext .js,.ts,.tsx --fix",
    "nuke": "pnpx rimraf node_modules",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@codemirror/commands": "^6.2.4",
    "@codemirror/lang-markdown": "^6.1.1",
    "@codemirror/language-data": "^6.3.1",
    "@codemirror/state": "^6.2.1",
    "@codemirror/view": "^6.12.0",
    "@noteup/shared": "workspace:0.4.3",
    "@radix-ui/react-context-menu": "^2.1.4",
    "@radix-ui/react-dropdown-menu": "^2.0.5",
    "@radix-ui/react-popover": "^1.0.6",
    "@tauri-apps/api": "^1.3.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@uiw/codemirror-theme-duotone": "^4.20.2",
    "@uiw/codemirror-theme-github": "^4.20.2",
    "@uiw/codemirror-theme-xcode": "^4.20.2",
    "@uiw/react-codemirror": "^4.20.2",
    "axios": "^1.4.0",
    "codemirror": "^6.0.1",
    "dayjs": "^1.11.7",
    "file-saver": "^2.0.5",
    "jspdf": "^2.5.1",
    "mousetrap": "^1.6.5",
    "mousetrap-global-bind": "^1.1.0",
    "patch-package": "^7.0.0",
    "postinstall-postinstall": "^2.1.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-markdown": "^8.0.7",
    "react-router-dom": "^6.11.2",
    "react-split-pane": "^0.1.92",
    "react-syntax-highlighter": "^15.5.0",
    "recoil": "^0.7.7",
    "recoil-persist": "^4.2.0",
    "rehype-raw": "^6.1.1",
    "remark-breaks": "^3.0.3",
    "remark-gfm": "^3.0.1",
    "remark-parse": "^10.0.2",
    "styled-components": "^5.3.11",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.4",
    "uuid": "^9.0.0",
    "web-vitals": "^3.3.2"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.22.3",
    "@tauri-apps/cli": "^1.3.1",
    "@testing-library/react": "^14.0.0",
    "@types/codemirror": "^5.60.7",
    "@types/file-saver": "^2.0.5",
    "@types/mousetrap": "^1.6.11",
    "@types/node": "^20.2.5",
    "@types/react": "^18.2.7",
    "@types/react-dom": "^18.2.4",
    "@types/react-syntax-highlighter": "^15.5.7",
    "@types/styled-components": "^5.1.26",
    "@types/uuid": "^9.0.1",
    "@vitejs/plugin-react": "^4.0.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest-extended": "^3.2.4",
    "jest-raw-loader": "^1.0.1",
    "jest-styled-components": "^7.1.1",
    "vite": "^4.3.9",
    "vite-plugin-pwa": "^0.15.2",
    "vite-tsconfig-paths": "^4.2.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ],
    "**/*.{json,md}": [
      "prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "keywords": [
    "notes",
    "notes-app",
    "note-taking",
    "markdown",
    "markdown-editor",
    "markdown-preview",
    "typescript",
    "tauri",
    "tauri-app"
  ]
}
